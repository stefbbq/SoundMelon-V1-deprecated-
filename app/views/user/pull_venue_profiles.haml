= render :partial => "/fan/live"
.primary-container
  .primary
    .left
      - if @accessible_venues.blank?
        You have no venue profiles associated with your account, would you like to #{link_to 'create one?', new_venue_path, :class => 'ajaxopen backable', :remote => true}
      - else
        - @accessible_venues.each do |venue|
          = render '/venue/venue_item', :venue =>venue

        #{link_to 'create another venue profile?', new_venue_path, :class => 'ajaxopen backable', :remote => true} You can add another venue profile to associate with your account!
    .right
      = render "/fan/right", :current =>'access_venue'