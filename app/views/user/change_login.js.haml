- if @is_fan  
  $('#page-content').html('#{escape_javascript(render '/fan/live')}');
  $('#page-content').append('<div class="primary-container"></div>');
  $('.primary-container').append('<div class="primary"></div>');
  $('.primary').append('<div class="left"><div class="top"></div></div>');
  $('.primary .left .top').append('#{escape_javascript(render '/user_posts/status_fan')}');
  $('.primary .left .top').append('#{escape_javascript(render '/fan/social_header')}');
  $('.primary .left').append('#{escape_javascript(render '/bricks/fan_posts')}');
  $('.primary').append('<div class="right"></div>');
  $('.primary .right').html('#{escape_javascript(render '/fan/right')}');
  setTimeout('$(document).trigger(\"close.facebox\")',10);
  $('.selection-box').removeClass('active').removeClass('button-fill-fan');
  $('#feed-msg').addClass('active').addClass('button-fill-fan');
  $('.left a').removeClass('active').removeClass('button-fill-fan');
  $('#latest').addClass('active').addClass('button-fill-fan');
- elsif @is_artist  
  $('.live').replaceWith('#{escape_javascript(render '/artist/live')}');
  $('.primary .left').html('<div class="top"></div>');
  $('.primary .left .top').html('#{escape_javascript(render '/user_posts/status_artist')}');
  $('.primary .left .top').append('#{escape_javascript(render '/artist/social_header', :current=>'mention')}');
  $('.primary .left').append('#{escape_javascript(render '/bricks/artist_posts')}');
  $('.primary .right').html('#{escape_javascript(render '/artist/right')}');
  setTimeout('$(document).trigger(\"close.facebox\")',10);
  $('.selection-box').removeClass('active').removeClass('button-fill-artist');
  $('#feed-msg').addClass('active').addClass('button-fill-artist');
  - if @first_login
    artistTutorial();
- elsif @is_venue
  $('.live').replaceWith('#{escape_javascript(render '/venue/live')}');
  $('.primary .left').html('<div class="top"></div>');
  $('.primary .left .top').html('#{escape_javascript( render '/user_posts/status_venue')}');
  $('.primary .left .top').append('#{escape_javascript(render '/venue/social_header')}');
  $('.primary .left').append('#{escape_javascript(render '/bricks/artist_posts')}');
  $('.primary .right').html('#{escape_javascript(render '/venue/right')}');
  setTimeout('$(document).trigger(\"close.facebox\")',10);
  $('.selection-box').removeClass('active').removeClass('button-fill-artist');
  $('#feed-msg').addClass('active').addClass('button-fill-artist');
history.pushState({sm:true}, document.title, '/home');
$('.top-bar .session .summary').html('#{escape_javascript(render '/bricks/session_header')}');
$('.session .notifications').html('#{escape_javascript(render '/bricks/notifications')}');