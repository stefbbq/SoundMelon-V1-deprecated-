- venue_albums = capture do
  #photo_album_box
    - if @has_link_access
      .content-actions
        = link_to "add album", new_venue_album_path('home'), :remote=>:true, :class=>"ajaxload add-new"
      - if @album_list.empty?
        %img{:src => "/assets/tutorial/artist-instruct-photos.png", :class => "instruct-img"}
    - @album_list.each do |album|
      = render '/venue_photo/album', :album =>album, :show_all =>@show_all || false
updateLiveSection('#{escape_javascript(render '/venue/live')}');
updateLeftSection('#{escape_javascript venue_albums}');
updateRightSection('#{escape_javascript(render '/venue/right', :current => 'photo')}');
closeFacebox(1);
scrollToTop();