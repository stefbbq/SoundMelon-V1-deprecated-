%h3 Choose your favorite genres
#location_info.content-box
  = form_for(:user, {:url => profile_item_setup_path, :remote => true}) do |ff|
    = hidden_field_tag :setup_item_type, '1'
    .content-box
      .area.form
        .details
          Start Typing
        .content
          .field
            %br
            = text_field_tag :fan_genres,'',{ :size => 40, :class => 'textfield'}
            %br
      = ff.submit 'skip this', :class => 'button'
      = ff.submit 'conitnue', :class => 'button'      

:javascript
  $("#fan_genres").tokenInput(
    [#{genre_atuofill}],
    {
      preventDuplicates: true,
      theme: "facebook",
      prePopulate: [#{genre_prepopullate(@user.genres)}]
  });
