= render :partial=>"shared/user_header_bar"
= render :partial=>"profile/user_dash_board"
.midcontent
  .left
    .leftcontainer
      = render 'users/post_msg_header'
      = render 'messages/inbox_messages', :messages => @messages
  .right
    = render "users/user_right_box"
  .clear
:javascript
  $('a').removeClass('highlight');
  $('#messages').addClass('highlight');