= render :partial=>"live"
.primary-container
  .primary
    .left
      %b Members of '#{@band.name}'
      %hr
      - @band_members.each do |user|
        .band-member
          .avatar
            = link_to get_avatar_medium(user), fan_profile_path(user.id), :class =>'ajaxopen bacable'
          .user-name
            %strong
              = link_to user.get_full_name, fan_profile_path(user.id), :class =>'ajaxopen bacable'
          .clear
%br
%br