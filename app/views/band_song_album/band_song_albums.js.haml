- song_albums = capture do
  - if @has_admin_access
    = render 'featured_songs', :featured_songs =>@band.featured_songs
  .clear
  .album-list
    - if @has_admin_access
      = link_to "add album", new_band_song_album_path(:band_name => @band.name), :remote=>:true, :class=>"ajaxopen add-album", :title =>'add new album'    
    - @artist_song_albums.each do |song_album|
      = render 'band_song_album', :song_album =>song_album, :show_all =>@show_all || false
$('.primary .left').html('#{escape_javascript song_albums}'); 
setTimeout('$(document).trigger(\"close.facebox\")',10);
$('.selection-box').removeClass('active').removeClass('button-fill-artist')
$('#music').addClass('active').addClass('button-fill-artist');